[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  go_up = <Super>Up
  go_down = <Super>Down
  go_left = <Super>Left
  go_right = <Super>Right
  move_tab_right = <Primary><Shift>Right
  move_tab_left = <Primary><Shift>Left
  next_tab = <Shift>Right
  prev_tab = <Shift>Left
  reset_clear = <Primary><Shift>k
  preferences_keybindings = <Primary><Shift>j
[profiles]
  [[default]]
    background_color = "#282828"
    foreground_color = "#ebdbb2"
    scrollback_lines = 99999
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
